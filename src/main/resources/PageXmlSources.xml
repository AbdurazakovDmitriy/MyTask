<ModelType>
    <Drivers>
        <Driver type="ChromeDriver" filePath="D:\chromedriver\chromedriver.exe" key="webdriver.chrome.driver"/>
        <Driver type="FirefoxDriver" filePath="D:\firefoxdriver\geckodriver.exe" key="webdriver.gecko.driver"/>
    </Drivers>
    <Pages>
        <Page name="LoginPage">
            <Element name="close" type="WebElement" ChromePath="//span[@class='RveJvd snByac']"
                     FirefoxPath="//div[@ng-click='close()']"/>
            <Element name="open" type="WebElement" ChromePath="//span[@class='RveJvd snByac']"
                     FirefoxPath="//div[@ng-click='open()']"/>
            <Element deprecated="true" name="info" type="Label" ChromePath="//div[@class='info']"
                     FirefoxPath="//div[@class='info']"/>
        </Page>
        <Page name="AnotherPage">
            <Element name="rs_hint" type="Label" ChromePath="//div[@ng-click='close()']"
                     FirefoxPath="//div[@ng-click='close()']"/>
            <Element name="link" type="Button" ChromePath="//div[@class='info']"
                     FirefoxPath="//div[@class='info']"/>
        </Page>
    </Pages>
</ModelType>
